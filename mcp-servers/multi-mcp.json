{
  "mcpServers": {
    "tavily-mcp": {
      "command": "npx",
      "args": ["-y", "tavily-mcp@0.1.3"],
      "env": {
        "TAVILY_API_KEY": "<TAVILY_API_KEY>"
      },
      "disabled": false,
      "autoApprove": []
    },
    "notionApi": {
      "command": "npx",
      "args": ["-y", "@notionhq/notion-mcp-server"],
      "env": {
        "OPENAPI_MCP_HEADERS": "{\"Authorization\": \"Bearer <NOTION_BEARER_TOKEN>\", \"Notion-Version\": \"2022-06-28\" }"
      }
    }
    
  }
}
